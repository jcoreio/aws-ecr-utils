{
  "name": "@jcoreio/aws-ecr-utils",
  "version": "0.0.0-development",
  "description": "utilities for working with AWS Elastic Container Registry",
  "sideEffects": false,
  "scripts": {
    "tc": "toolchain",
    "toolchain": "toolchain",
    "test": "toolchain test",
    "prepublishOnly": "echo This package is meant to be published by semantic-release from the dist build directory. && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/aws-ecr-utils.git"
  },
  "keywords": [
    "aws",
    "ecr",
    "aws-sdk"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/aws-ecr-utils/issues"
  },
  "homepage": "https://github.com/jcoreio/aws-ecr-utils#readme",
  "devDependencies": {
    "@jcoreio/toolchain": "^5.8.8",
    "@jcoreio/toolchain-circle": "^5.8.8",
    "@jcoreio/toolchain-esnext": "^5.8.8",
    "@jcoreio/toolchain-mocha": "^5.8.8",
    "@jcoreio/toolchain-semantic-release": "^5.8.8",
    "@jcoreio/toolchain-typescript": "^5.8.8",
    "@types/chai": "^4.3.5",
    "@types/inquirer": "^8.2.10",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.10.2",
    "chai": "^4.3.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^8.3.0",
    "globals": "^16.0.0",
    "lint-staged": "^15.2.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.577.0",
    "@aws-sdk/client-sts": "^3.577.0",
    "@babel/runtime": "^7.26.0",
    "base64-js": "^1.5.1",
    "find-up": "^8.0.0",
    "inquirer": "^8.2.6",
    "log4jcore": "^4.8.0",
    "promisify-child-process": "^5.0.0",
    "zod": "^3.25.76"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "default": "./dist/index.d.ts"
      },
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": {
        "import": "./dist/*.d.mts",
        "default": "./dist/*.d.ts"
      },
      "require": "./dist/*.js",
      "default": "./dist/*.mjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@10.6.5",
  "@jcoreio/toolchain": {
    "migratedVersion": "5.8.8"
  }
}
