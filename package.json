{
  "name": "@jcoreio/aws-ecr-utils",
  "version": "0.0.0-development",
  "description": "utilities for working with AWS Elastic Container Registry",
  "sideEffects": false,
  "scripts": {
    "tsc:watch": "npm run tsc -- --watch",
    "tc": "toolchain",
    "toolchain": "toolchain",
    "test": "toolchain test",
    "prepublishOnly": "echo This package is meant to be published by semantic-release from the dist build directory. && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/aws-ecr-utils.git"
  },
  "keywords": [
    "aws",
    "ecr",
    "aws-sdk"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/aws-ecr-utils/issues"
  },
  "homepage": "https://github.com/jcoreio/aws-ecr-utils#readme",
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@jcoreio/toolchain": "^3.2.2",
    "@jcoreio/toolchain-circle": "^3.2.2",
    "@jcoreio/toolchain-esnext": "^3.2.2",
    "@jcoreio/toolchain-mocha": "^3.2.2",
    "@jcoreio/toolchain-semantic-release": "^3.2.2",
    "@jcoreio/toolchain-typescript": "^3.2.2",
    "@types/chai": "^4.3.5",
    "@types/inquirer": "^8.2.10",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@typescript-eslint/typescript-estree": "^4.32.0",
    "chai": "^4.3.7",
    "copy": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.577.0",
    "@aws-sdk/client-sts": "^3.577.0",
    "@babel/runtime": "^7.18.6",
    "base64-js": "^1.5.1",
    "inquirer": "^8.2.6",
    "is-interactive": "^1.0.0",
    "promisify-child-process": "^4.1.1",
    "zod": "^3.22.4"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.mjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.11.0"
}
